{
    "compilerOptions": {
        "moduleResolution": "Node",
        "target": "ESNext",
        "lib": ["DOM", "ESNext"],
        "importsNotUsedAsValues": "error",
        "preserveValueImports": true,
        "isolatedModules": true,
        "sourceMap": true,
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}
